      "executionTemplate" : [
        "",
        "# Combine the two \"file path attribute\" parameters into one value.",
        "DEFAULT_MACRO $(XFORMER_NAME)_FILE_PATH_ATTR",
        "",
        "INCLUDE [",
        "\tif { {$(FILE_PATH_ATTR)} == {<Unused>} } {",
        "\t\tputs {MACRO $(XFORMER_NAME)_FILE_PATH_ATTR \"$(FILE_PATH_ATTR2)\"};",
        "\t} else {",
        "\t\tputs {MACRO $(XFORMER_NAME)_FILE_PATH_ATTR \"$(FILE_PATH_ATTR)\"};",
        "\t}",
        "]",
        "",
        "FACTORY_DEF {*} HTTPFactory",
        "   FACTORY_NAME { $(XFORMER_NAME) }",
        "      $(INPUT_LINES)",
        "   TARGET_URL { \"$(TARGET_URL)\" }",
        "   HTTP_METHOD { $(HTTP_METHOD) }",
        "   SAVE_FILE { $(SAVE_FILE_LOOKUP) }",
        "   OUTPUT_FILENAME { \"$(OUTPUT_FILENAME)\" }",
        "   OUTPUT_DIRECTORY { \"$(OUTPUT_DIRNAME)\" }",
        "   FILE_EXTENSION { \"$(FILE_TYPE_LOOKUP)\" }",
        "   TARGET_ATTR { \"$(TARGET_ATTR)\" }",
        "   FILE_PATH_ATTR { $[$(XFORMER_NAME)_FILE_PATH_ATTR] }",
        "   UPLOAD_FILE { \"$(UPLOAD_FILE)\" }",
        "   UPLOAD_BODY { \"$(UPLOAD_BODY)\" }",
        "   UPLOAD_TYPE { $(UPLOAD_TYPE) }",
        "   UPLOAD_CONTENT_TYPE { \"$(CONTENT_TYPE_LOOKUP)\" }",
        "   COMBINED_MULTIPART_TABLE { $(MULTIPARTS) }",
        "   MULTIPART_CONTENT_TYPE { $(MULTIPART_CONTENT_TYPE) }",
        "   REQUEST_HEADER_TABLE { $(CUSTOM_HEADERS) }",
        "   ADDITIONAL_URL_PARAMETERS { $(ADDITIONAL_URL_PARAMETERS) }",
        "   AUTH_USERNAME { \"$(AUTH_USERNAME)\" }",
        "   AUTH_PASSWORD { \"$(AUTH_PASSWORD)\" }",
        "   AUTH_METHOD { \"$(AUTH_METHOD_TYPE)\" }",
        "   TARGET_ATTRIBUTE_ENCODING { $(TARGET_ATTR_ENCODING) }",
        "   RESPONSE_HEADER_LIST_ATTR { \"$(RESPONSE_HEADER_LIST_ATTR)\" }",
        "   STATUS_CODE_ATTR { \"$(STATUS_CODE_ATTR)\" }",
